type Drop {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  text: String!
  author: User
  location: Location!
  color: String!
}

type User {
  id: ID! @unique
  email: String! @unique
  password: String!
  username: String! @unique
  drops: [Drop!]!
}

type Location {
  latitude: Float!
  longitude: Float!
  altitude: Float!
}